{
  "name": "static-website",
  "private": true,
  "description": "HarvestDash Official Website",
  "version": "0.1.0",
  "license": "LGPL-3.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --ext .js src/",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "emotion-theming": "^10.0.14",
    "gatsby": "^2.13.83",
    "gatsby-image": "^2.2.14",
    "gatsby-plugin-manifest": "^2.2.9",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-sharp": "^2.2.12",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "rebass": "^4.0.5",
    "typeface-inter": "^0.0.77"
  },
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.4",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HarvestDash/static-website/"
  },
  "bugs": {
    "url": "https://github.com/HarvestDash/static-website/issues"
  }
}
